<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Frid-G-Box</name>
    <description>
        Frid-G-Box recipe application for phone.
    </description>
    <author email="ainakiurus@gmail.com" href="https://github.com/MissAlways/Frid-G-Box">
        MissAlways
    </author>
    <content src="index.html" />
    <access origin="*" />
	<plugin name="cordova-plugin-whitelist" version="1" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
</widget>
